{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**"],
      "restart": true,
      "protocol": "inspector",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
